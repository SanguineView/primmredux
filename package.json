{
  "name": "starting-line",
  "version": "1.0.0",
  "description": "The starting line",
  "main": "index.js",
  "scripts": {
    "watch": "node-sass assets/sass/style.scss style.css -w",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' style.css -o style.prefix.css",
    "compress:css": "node-sass style.prefix.css style.min.css --output-style compressed",
    "build": "npm-run-all prefix:css compress:css"
  },
  "author": "Author",
  "license": "ISC",
  "devDependencies": {
    "@glidejs/glide": "^3.4.1",
    "autoprefixer": "^9.7.4",
    "basscss": "^8.0.2",
    "concat": "^1.0.3",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "frow": "^3.3.2",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-hexrgba": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.1",
    "postcss-simple-vars": "^5.0.2",
    "tailwindcss": "^1.2.0"
  },
  "dependencies": {}
}
